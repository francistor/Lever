{
    "__COMMENT": "Use transport names and properties as specified in https://github.com/winstonjs/winston/blob/master/docs/transports.md",

    "_messageLogLevel": "verbose",

    "console":{
        "type": "Console",
        "properties": {"timestamp": true, "level": "info"}
    },

    "_file":{
        "type": "DailyRotateFile",
        "properties": {"datePattern": ".yyyy-MM-dd", "timestamp": true, "level": "info", "filename": "log/performance-client.log", "json": false}
    }
}